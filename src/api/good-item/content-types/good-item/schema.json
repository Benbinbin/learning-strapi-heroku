{
  "kind": "collectionType",
  "collectionName": "good_items",
  "info": {
    "singularName": "good-item",
    "pluralName": "good-items",
    "displayName": "GoodItem",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "itemId": {
      "type": "uid",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "abstract": {
      "type": "text"
    },
    "customizable": {
      "type": "boolean",
      "default": true
    },
    "stock": {
      "type": "integer",
      "default": 0
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "bag",
        "shoe",
        "cloth"
      ]
    },
    "goodStyle": {
      "type": "component",
      "repeatable": false,
      "component": "utilities.good-style"
    },
    "good_description": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::good-description.good-description",
      "mappedBy": "good_item"
    },
    "variants": {
      "type": "component",
      "repeatable": true,
      "component": "utilities.good-variant",
      "required": true
    }
  }
}
